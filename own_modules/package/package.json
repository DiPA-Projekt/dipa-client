{
  "name": "@kolibri/react",
  "version": "1.0.0-beta.13",
  "license": "Apache-2.0",
  "description": "React spezifischer wrapper fÃ¼r @kolibri/lib.",
  "scripts": {
    "build": "npm run clear && npm run compile",
    "reinstall": "npm run remove && npm install",
    "reinstall:kolibri": "npm cache clear -f && rm -rf node_modules/@kolibri package-lock.json && npm i",
    "remove:lock": "rm -rf package-lock.json",
    "remove": "npm run remove:lock && git clean -f -d -X",
    "republish": "npm unpublish -f && npm run build && npm publish",
    "clear": "rm -rf dist",
    "compile": "npm run tsc",
    "tsc": "tsc -p .",
    "update": "ncu -t minor -u && npm install && ncu",
    ":publish": "npm unpublish -f && npm publish",
    ":sync": "rm -rf node_modules/@kolibri && npm i"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "dependencies": {
    "@kolibri/lib": "1.0.0-beta.13"
  },
  "devDependencies": {
    "@leanup/form": "1.2.0-rc.118",
    "@types/node": "16.7.8",
    "@types/react": "17.0.19",
    "@types/react-dom": "17.0.9",
    "npm-check-updates": "11.8.3",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "tsc": "2.0.3",
    "typescript": "^4.4.2"
  },
  "peerDependencies": {
    "@leanup/form": "1.2.0-rc.118",
    "react": ">=16.14.0",
    "react-dom": ">=16.14.0"
  },
  "files": [
    "dist/"
  ],
  "publishConfig": {
    "access": "public"
  }
}
